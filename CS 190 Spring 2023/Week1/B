#include <stdio.h>
#include <vector>
#include <iostream>
#include <iomanip>
#include <cmath>

using namespace std;

int main()
{
    cout << fixed;
    cout << setprecision(12);
    float p1,p2,x,y,d;
    cin >> p1 >> p2;
    d = sqrt(pow(p1,2)+pow(p2,2));
    x = p1/d;
    y = p2/d;
    cout << x << " " << y;
    return 0;
}
